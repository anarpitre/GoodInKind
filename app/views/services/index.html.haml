- content_for :header do
  = javascript_include_tag 'service', 'filter'
.sidebar_bar
  = render :partial => "service_search"
  .sidebar_left_find
    = render :partial => 'category_filter'
    = render :partial => 'nonprofit_filter'
    = render :partial => 'price_filter'
    = render :partial => 'date_filter'
.featured_services_find
  %h2.fs_find_h2
    Popular results
    %br/
    %span
      Don't find what you are looking for?
      %a{:href => "#"} Place your request here
  .featured_list_find{:id => 'service_list'}
%p.view_more
  %a{:href => "#"} View More Services
:javascript
  var services = #{@services.to_json};
