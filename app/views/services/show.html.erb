<p id="notice"><%= notice %></p>

<p>
  <b>Title:</b>
  <%= @service.title %>
</p>

<p>
  <b>Description:</b>
  <%= @service.description %>
</p>

<p>
  <b>Amount:</b>
  <%= @service.amount %>
</p>

<p>
  <b>Booking capacity:</b>
  <%= @service.booking_capacity %>
</p>


<p>
  <b>Is scheduled:</b>
  <%= @service.is_scheduled %>
</p>

<p>
  <b>Start date:</b>
  <%= @service.start_date.blank? ? '' : @service.start_date.strftime('%m/%d/%Y') %>
</p>

<p>
  <b>End date:</b>
  <%= @service.end_date.blank? ? '' : @service.end_date.strftime('%m/%d/%Y')%>
</p>

<p>
  <b>Start time:</b>
  <%= (@service.start_time.blank? ? '' : @service.start_time.strftime('%I:%M%p')) %>
</p>

<p>
  <b>End time:</b>
  <%= @service.end_time.blank? ? '' : @service.end_time.strftime("%I:%M%p") %>
</p>

<p>
  <b>Non profit:</b>
  <%= @service.non_profit_id %>
</p>



<p>
  <b>Is virtual:</b>
  <%= @service.is_virtual %>
</p>

<p>
  <b>Is public:</b>
  <%= @service.is_public %>
</p>


<%= link_to 'Edit', edit_service_path(@service) %> |
<%= link_to 'Back', services_path %>
